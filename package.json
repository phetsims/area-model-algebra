{
  "name": "area-model-algebra",
  "version": "1.3.2",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/area-model-algebra.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "AREA_MODEL_ALGEBRA",
